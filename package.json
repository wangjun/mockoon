    "@angular/router": "8.2.14",
    "@apidevtools/swagger-parser": "9.0.1",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/cookie-parser": "1.4.2",
    "@types/electron-json-storage": "4.0.0",
    "@types/express": "4.17.3",
    "@types/faker": "4.1.11",
